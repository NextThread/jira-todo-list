{
  "name": "jira-todo-list",
  "private": true,
  "version": "0.0.2",
  "description": "",
  "repository": "",
  "license": "MIT",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"NODE_ENV=development npm run watch\" \"npm run serve\" \"npm run sync\"",
    "prod": "npm run build:prod && npm run serve",
    "watch": "webpack --watch --config webpack.config.js",
    "serve": "nodemon server/index.js --watch server -e html,js ",
    "sync": "browser-sync start --proxy \"localhost:3002\" --files \"client/dist/**/*\"",
    "build": "webpack --config webpack.config.js",
    "build:prod": "ENV=PROD webpack --config webpack.config.js",
    "nodemon": "nodemon server/index.js",
    "start": "forever server/index.js",
    "deploy": "npm run build:prod && git commit -am \"pushing to prod\" && git push heroku master",
    "force-deploy": "npm version patch && npm run deploy",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "profile": "webpack --profile --json > stats.json"
  },
  "dependencies": {
    "@shopify/draggable": "^1.0.0-beta.5",
